

/* 轮播图样式 */
.slide-image {
  width:750rpx;
  height:340rpx;
}

/* 分类菜单样式 */
.menus {
  display:flex;
  justify-content:space-around;
  padding:20rpx 0;
}

.menus image {
  width:128rpx;
  height:140rpx;
}

/* 楼层样式 */
.floor {
  margin-top:20rpx;
}
.floor-title image {
  width:100%;
  height:60rpx;
}
.floor-content {
  padding:20rpx;
  display:flex;
  justify-content:space-between;
}
.floor-content-left image{
  width:232rpx;
  height:386rpx;
}
.floor-content-right {
  flex:1;
  margin-left:10rpx;
  display:flex;
  justify-content: space-between;
  flex-wrap:wrap;
}
.floor-content-right image{
  width:233rpx;
  height:188rpx;
}
